{
  "name": "koa-better-serve",
  "version": "0.0.0-semantic-release",
  "description": "Small, simple and correct serving of files, using [koa-send][] - nothing more",
  "repository": "tunnckoCore/koa-better-serve",
  "homepage": "https://github.com/tunnckoCore/koa-better-serve",
  "author": "Charlike Mike Reagent <@tunnckoCore> (https://i.am.charlike.online)",
  "main": "./index.js",
  "license": "MIT",
  "scripts": {
    "lint": "eslint . --fix --format codeframe",
    "dev": "chokidar '*.js' -c 'npm test'",
    "test": "nyc --reporter lcov node test.js && nyc report",
    "precommit": "npm run lint && npm test && git add --all",
    "commit": "simple-commit-message",
    "postcommit": "git push",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post"
  },
  "engines": {
    "node": ">=7",
    "npm": ">=5"
  },
  "files": [
    "index.js"
  ],
  "dependencies": {
    "kind-of": "5.0.2",
    "koa-send": "4.1.0"
  },
  "devDependencies": {
    "chokidar-cli": "1.2.0",
    "eslint": "4.4.1",
    "eslint-config-standard": "10.2.1",
    "eslint-config-standard-jsx": "4.0.2",
    "eslint-config-standard-react": "5.0.0",
    "eslint-plugin-fp": "2.3.0",
    "eslint-plugin-import": "2.7.0",
    "eslint-plugin-jsdoc": "3.1.2",
    "eslint-plugin-node": "5.1.1",
    "eslint-plugin-promise": "3.5.0",
    "eslint-plugin-react": "7.1.0",
    "eslint-plugin-standard": "3.0.1",
    "github-post-release": "1.13.0",
    "husky": "0.14.3",
    "koa": "2.3.0",
    "mukla": "0.4.9",
    "nyc": "11.1.0",
    "semantic-release": "^6.3.6",
    "simple-commit-message": "3.3.1",
    "supertest": "3.0.0"
  },
  "keywords": [
    "content",
    "contents",
    "file",
    "files",
    "images",
    "index",
    "koa",
    "koa-send",
    "koajs",
    "middleware",
    "send",
    "serve",
    "serving",
    "static",
    "upload"
  ],
  "nyc": {
    "check-coverage": true,
    "statements": 100,
    "functions": 100,
    "branches": 100,
    "lines": 100
  },
  "renovate": {
    "enabled": true,
    "pinVersions": true,
    "rebaseStalePrs": true,
    "semanticCommits": true,
    "unpublishSafe": false,
    "labels": [
      "deps",
      "renovate"
    ],
    "prCreation": "not-pending",
    "depTypes": [
      {
        "depType": "dependencies",
        "semanticPrefix": "fix(renovate/deps): ",
        "automerge": "minor",
        "automergeType": "pr",
        "assignees": [
          "charlike"
        ]
      },
      {
        "depType": "devDependencies",
        "semanticPrefix": "chore(renovate/devDeps): ",
        "automerge": "any",
        "automergeType": "branch-push"
      }
    ],
    "packageRules": [
      {
        "packageNames": [
          "semantic-release"
        ],
        "automerge": "minor",
        "automergeType": "pr",
        "pinVersions": false
      }
    ]
  },
  "verb": {
    "run": true,
    "toc": {
      "render": true,
      "method": "preWrite",
      "maxdepth": 3
    },
    "layout": "empty",
    "tasks": [
      "readme"
    ],
    "related": {
      "list": [
        "dush",
        "koa-better-router",
        "koa-better-body",
        "koa-rest-router",
        "koa-bel",
        "micro"
      ],
      "highlight": "koa-better-router"
    },
    "reflinks": [
      "always-done",
      "async-done",
      "base",
      "browserify",
      "charlike",
      "charlike-cli",
      "commitizen",
      "dezalgo",
      "dush-better-use",
      "dush-methods",
      "dush-no-chaining",
      "dush-options",
      "dush-promise",
      "dush-router",
      "dush-tap-report",
      "hyperscript",
      "koa-send",
      "mitt",
      "once",
      "rollup",
      "standard-version",
      "verb",
      "verb-generate-readme",
      "webpack",
      "koa",
      "koa-convert",
      "path-match"
    ],
    "lint": {
      "reflinks": true
    }
  },
  "release": {
    "analyzeCommits": "simple-commit-message",
    "generateNotes": "github-post-release"
  }
}
